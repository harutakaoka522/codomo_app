<p id="notice"><%= notice %></p>
<div data-turbolinks="false">
<h1>Events</h1>
<div id="calendar"></div>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>Status</th>
      <th>Start at</th>
      <th>End at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.content %></td>
        <td><%= event.status %></td>
        <td><%= event.start_at.strftime("%Y-%m-%d %H:%M:%S") %></td>
        <td><%= event.end_at.strftime("%Y-%m-%d %H:%M:%S") %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</div>
<%= link_to 'New Event', new_event_path %>
