<!DOCTYPE html>
<html>
  <head>
    <title>Codomo</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <% if user_signed_in? %>
  <header>
 
  <nav>
  <ul>
    <li><a><%= link_to 'ホーム', user_path(current_user.id) %></a></li>
    <li><a><%= link_to 'スケジュール', events_path %></a></li>
    <li><a><%= link_to "ログアウト", destroy_user_session_path, method: :delete %></a></li>
  </ul>
</nav>
</header>
<% end %>


    <%= yield %>
</body>
</html>
